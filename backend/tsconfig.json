{
  "compilerOptions": {
    "types": ["node", "jest"],
    "module": "nodenext",
    "target": "esnext",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "include": ["src"]
}